<?xml version="1.0" encoding="utf-8" ?>
<configuration>
    <startup> 
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.7.2" />
    </startup>
    <system.serviceModel>
        <bindings>
            <basicHttpBinding>
                <binding name="BasicHttpBinding_IMathService" />
                <binding name="DecryptPortBinding" />
            </basicHttpBinding>
        </bindings>
        <client>
            <endpoint address="http://localhost:63429/MathService.svc" binding="basicHttpBinding"
                bindingConfiguration="BasicHttpBinding_IMathService" contract="MathService.IMathService"
                name="BasicHttpBinding_IMathService" />
            <endpoint address="http://localhost:9080/DecryptService/DecryptServiceBean"
                binding="basicHttpBinding" bindingConfiguration="DecryptPortBinding"
                contract="DecryptService.DecryptEndPoint" name="DecryptPort" />
        </client>
    </system.serviceModel>
</configuration>